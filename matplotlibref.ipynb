year  = [1972,    1982,   1992,   2002,   2012]
e_kar = [100.6,   158.6,  305.5,  394.9,  724.8]
e_goa = [10.5,    25.2,   58.6,   119.3,  274.8]

plt.plot(year,e_kar)
plt.plot(year,e_goa)

plt.figure(figsize=(14,8))
plt.plot(year,e_kar,color="#f56642",label="Karnataka",linestyle="dashed",marker="*")
plt.plot(year,e_goa,color="k",label="Goa",linestyle="dashdot",marker=".")
plt.ylabel("Electricity consumption")
plt.xlabel("Years",fontsize=14)
plt.legend(loc="upper left")
plt.title("Electricity Consumption Analysis",fontsize=18)
plt.xticks(np.arange(1970,2014,2),rotation=30)
